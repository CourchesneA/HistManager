{
  "manifest_version": 2,

  "name": "History manager",
  "description": "This extension will remove specific entries related to a set subject in the browser history",
  "version": "1.0",
  
  "options_page": "options.html",

  "browser_action": {
    "default_icon": "icon.png",
    "default_popup": "popup.html"
  },
  "permissions": [
    "history",
    "activeTab",
    "storage",
    "https://ajax.googleapis.com/"
  ]
}
